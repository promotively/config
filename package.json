{
  "name": "@promotively/config",
  "version": "1.0.0",
  "author": {
    "name": "Steven Ewing",
    "email": "steven.ewing@promotively.com"
  },
  "repository": {
    "type": "git",
    "url": "https://www.github.com/promotively/config"
  },
  "bugs": {
    "url": "https://github.com/promotively/config/issues"
  },
  "homepage": "https://www.promotively.com",
  "license": "MIT",
  "browser": "dist/lib/browser.js",
  "main": "dist/lib/server.js",
  "scripts": {
    "build": "babel src --out-dir dist/lib",
    "postbuild": "node scripts/postbuild.js",
    "docs": "esdoc -c esdoc.config.js",
    "lint": "eslint -c eslint.config.js *.config.js src/*.js test/*.js scripts/*.js",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "export PATH=\"./node_modules/.bin:$PATH\" yarn lint && yarn test"
    }
  },
  "description": "Universal/isomorphic javascript library for configuration file fetching.",
  "keywords": [
    "es2018",
    "es6",
    "javascript",
    "fetch",
    "axios",
    "promises",
    "async",
    "await",
    "universal",
    "isomorphic",
    "ssr",
    "server-side-rendering"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.3",
    "@promotively/eslint-config": "^1.1.4",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-preset-minify": "^0.5.1",
    "coveralls": "^3.0.4",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-importpath-plugin": "^1.0.2",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^6.0.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-promise": "^4.1.1",
    "fetch-mock": "^7.5.1",
    "husky": "^3.0.9",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^24.8.0",
    "jest-mock-console": "^1.0.0",
    "prepend-file": "^1.3.1"
  }
}
